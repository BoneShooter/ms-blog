{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "interface-name": false,
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "no-console": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "no-empty": false,
        "max-line-length": [
            false
        ],
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}